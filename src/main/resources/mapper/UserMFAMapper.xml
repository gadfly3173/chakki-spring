<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="vip.gadfly.chakkispring.mapper.UserMFAMapper">

    <select id="getSecretKeyByUserId" resultType="java.lang.String">
        SELECT um.secret FROM lin_user_mfa AS um WHERE um.user_id = #{userId} AND um.delete_time IS NULL
    </select>
</mapper>
